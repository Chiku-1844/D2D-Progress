#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n = 371;
    int original = n;
    int sum = 0;
    while(n>0)
    {
      int last =0;
      last = n%10;
      sum = sum + pow(last,3);
      n = n/10;
      
    }
    
    if(original == sum )
    {
        cout <<"Armstrong number";
    }
    else{
        cout <<"Not a Armstrong number";
    }
}
